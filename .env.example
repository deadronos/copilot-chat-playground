# Copy to .env and fill in a fine-grained PAT with "Copilot Requests" permission
GH_TOKEN=__PASTE_YOUR_TOKEN_HERE__

# Optional: Set to "false" to use CLI mode instead of SDK mode (default: SDK)
# USE_COPILOT_SDK=false

# Telemetry WebSocket endpoint used by frontend when telemetry is enabled
# Set to a WebSocket URL like ws://localhost:4000/telemetry
# Vite prefixes env variables exposed to client with VITE_
VITE_TELEMETRY_WS_URL=

# Enable telemetry collection in the frontend (set to "true" or "false")
VITE_ENABLE_TELEMETRY=false
# Example model to use by default in Copilot Chat
COPILOT_DEFAULT_MODEL=gpt-5-mini